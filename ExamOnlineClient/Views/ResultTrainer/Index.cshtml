
@{
    ViewData["Title"] = "Result";
    Layout = "~/Views/Layout/_Layout.cshtml";
}

<div class="content-wrapper">
    <!-- START PAGE CONTENT-->

    
    <div class="page-heading">
        <h1 class="page-title">Exam Result</h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/"><i class="la la-home font-20"></i></a>
            </li>
        </ol>
    </div>

    <div class="page-content fade-in-up">
        <div class="ibox">
            @*<div class="ibox-head">
                <div class="ibox-title">Data Table</div>
                </div>*@
            <div class="ibox-body">

                <table class="table table-striped table-bordered table-hover" id="TblExam" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Employee Id</th>
                            <th>Subject Name</th>
                            <th>Examination Date</th>
                            <th>Score</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <!-- END PAGE CONTENT-->
</div>

@section Scripts{

    <script src="~/js/Script/ResultScript.js"></script>

}


