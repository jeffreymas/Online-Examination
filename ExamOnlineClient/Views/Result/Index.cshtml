@using Microsoft.AspNetCore.Http;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Layout/_Layout.cshtml";
}

<div class="content-wrapper">


    <div class="page-heading">
        <h1 class="page-title" align="center"><b>Exam Result</b></h1>
        <h4 class="page-title" align="center">@Context.Session.GetString("name")</h4>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/"><i class="la la-home font-20"></i></a>
            </li>
        </ol>
    </div>
    <div class="page-content fade-in-up">
        <div class="ibox">
            @*<div class="ibox-head">
                <div class="ibox-title">Data Table</div>
                </div>*@
            <div class="ibox-body">

                <table class="table table-striped table-bordered table-hover" id="TableResult" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Subject</th>
                            <th>Exam Date</th>
                            <th>Score</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <!-- END PAGE CONTENT-->
</div>

@section Scripts{
    <script src="~/js/Script/Result.js"></script>
}